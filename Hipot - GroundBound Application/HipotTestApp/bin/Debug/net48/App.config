<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
  </configSections>
  
  <connectionStrings>
    <!-- Remote connection to SQL Express on usyouwhp6205605 server -->
    <add name="PhoenixConnectionString" 
         connectionString="Data Source=usyouwhp6205605\SQLEXPRESS;Initial Catalog=Phoenix;Integrated Security=True;Connection Timeout=30;" 
         providerName="System.Data.SqlClient" />
    <!-- Connection to TestEngineering database for Users table -->
    <add name="TestEngineeringConnectionString" 
         connectionString="Data Source=usyouwhp6205605\SQLEXPRESS;Initial Catalog=TestEngineering;Integrated Security=True;Connection Timeout=30;" 
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <appSettings>
    <!-- GPIB Settings for OMNIA II 8204 via NI GPIB-USB-HS+ -->
    <!-- Use NI MAX to confirm your GPIB board name and device address -->
    <add key="OMNIA_GPIB_Board" value="GPIB1" />
    <add key="OMNIA_GPIB_Address" value="8" />
    <!-- Full VISA resource string: {Board}::{Address}::INSTR = GPIB1::8::INSTR -->
    
    <!-- Serial Port Settings (legacy - not used with GPIB-USB adapter) -->
    <!-- <add key="OMNIA_PortName" value="COM3" /> -->
    <!-- <add key="OMNIA_BaudRate" value="9600" /> -->
    
    <!-- Equipment Identification -->
    <add key="EquipmentID" value="OMNIA-8204-001" />
    <add key="EquipmentModel" value="Associated Research OMNIA II 8204" />
    
    <!-- Shift Configuration -->
    <add key="Shift1Start" value="06:00" />
    <add key="Shift1End" value="14:00" />
    <add key="Shift2Start" value="14:00" />
    <add key="Shift2End" value="22:00" />
    <add key="Shift3Start" value="22:00" />
    <add key="Shift3End" value="06:00" />
    
    <!-- Test Settings -->
    <add key="RequireSafetyCheckPerShift" value="true" />
    <add key="SafetyCheckExpirationHours" value="8" />
    
    <!-- OMNIA Test File Numbers -->
    <!-- These correspond to files stored on the OMNIA device -->
    <add key="MainHipotTestFile" value="1" />
    <add key="FailCheckGroundBondFile" value="3" />
    <add key="FailCheckACHipotFile" value="4" />
    
    <!-- Default Test Parameters (for reference/validation) -->
    <!-- Ground Bond Test Defaults -->
    <add key="GND_Current_A" value="25.00" />
    <add key="GND_HiLimit_mOhm" value="100" />
    <add key="GND_LoLimit_mOhm" value="0" />
    <add key="GND_DwellTime_s" value="1.0" />
    <add key="GND_Frequency_Hz" value="60" />
    
    <!-- AC Withstand Test Defaults -->
    <add key="ACW_Voltage_V" value="1960" />
    <add key="ACW_HiLimit_mA" value="35.00" />
    <add key="ACW_LoLimit_mA" value="0.001" />
    <add key="ACW_DwellTime_s" value="60.0" />
    <add key="ACW_RampUp_s" value="5.0" />
    <add key="ACW_RampDown_s" value="5.0" />
    <add key="ACW_Frequency_Hz" value="60" />
    <add key="ACW_ArcSense" value="8" />
    
    <!-- Logging -->
    <add key="LogEnabled" value="true" />
    <add key="LogPath" value="Logs" />
    
    <!-- TDM (Test Data Management) Configuration -->
    <!-- These settings define the TDM station hierarchy for uploading test results -->
    <add key="TDM_LineName" value="Phoenix" />
    <add key="TDM_ParentWorkStation" value="Phoenix Hipot Test" />
    <add key="TDM_WorkstationName" value="Phoenix Hipot Test - 1" />
    <add key="TDM_ShiftName" value="Shift 1" />
    <add key="TDM_TestSequenceName" value="Hipot_GND_ACW" />
    <add key="TDM_Enabled" value="true" />
  </appSettings>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
</configuration>
